{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "dist",
        "lib": ["ES2020"],
        "sourceMap": true,
        "rootDir": ".",
        "strict": true,
        "useUnknownInCatchVariables": false,
        "esModuleInterop": true,
        "moduleResolution": "nodenext",
        "resolveJsonModule": true,
        "paths": {
			"vs/*": [
				"./src/vs/*"
			]
		},
    },
    "declaration": true,
    "exclude": ["node_modules"]
}
